<ul class="errors"></ul>
<h1>Add a Course!</h1>
<form>
  <label for="course_name">Name</label>
  <input id="course_name" type="text" name="course[name]"></input>

  <br>

  <table>


    <tr>
      <td>Front Nine</td>
      <% for (var num = 1; num <= 9; num++) { %>
        <td><%= num %></td>
      <% } %>
    </tr>

    <tr>
      <td>Par</td>
      <% for (var num = 1; num <= 9; num++) { %>
        <td><input type="number" min="1" name="course[hole<%=num%>]" value= ""></td>
      <% } %>
    </tr>

    <tr>
      <td>Back Nine</td>
      <% for (var num = 10; num <= 18; num++) { %>
        <td><%= num %></td>
      <% } %>
    </tr>

    <tr>
      <td>Par</td>
      <% for (var num = 10; num <= 18; num++) { %>
        <td><input type="number" min="1" name="course[hole<%=num%>]" value= ""></td>
      <% } %>
    </tr>
  </table>

  <br>

  <% for (var num = 1; num <= 3; num ++) { %>
    <input id="imgInp" type="file" accept="image/*" data-key="hole_img<%=num%>" name="course[hole_img<%=num%>]">
    <img id="blah" src="#" alt="your image" />
  <% } %>

  <br>

  <input type="submit">


</form>
