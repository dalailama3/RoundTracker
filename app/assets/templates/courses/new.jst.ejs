<ul class="errors"></ul>
<h1>Add a Course!</h1>
<form>
  <label for="course_name">Name</label>
  <input id="course_name" type="text" name="course[name]"></input>

  <br>

  <table class="table">
    <tr>
      <td>Front</td>
      <% for (var num = 1; num <= 9; num++) { %>
        <td><%= num %></td>
      <% } %>

      <td>Back</td>
      <% for (var num = 10; num <= 18; num++) { %>
        <td><%= num %></td>
      <% } %>
    </tr>

    <tr class="pars">
      <td>Par</td>
      <% for (var num = 1; num <= 9; num++) { %>
        <td class="par" contenteditable="true" name="hole<%=num%>" value= ""></td>
      <% } %>


      <td></td>
      <% for (var num = 10; num <= 18; num++) { %>
        <td class="par" contenteditable="true" name="hole<%=num%>" value= ""></td>
      <% } %>
    </tr>
  </table>

  <br>


  <input type="submit">
</form>
